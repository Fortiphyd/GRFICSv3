auto eth0
iface eth0 inet static
    address 192.168.95.10
    netmask 255.255.255.0
    gateway 192.168.95.1

    up ip addr add 192.168.95.11/24 dev eth0
    up ip addr add 192.168.95.12/24 dev eth0
    up ip addr add 192.168.95.13/24 dev eth0
    up ip addr add 192.168.95.14/24 dev eth0
    up ip addr add 192.168.95.15/24 dev eth0
